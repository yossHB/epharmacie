    "release": "yarn build:once && yarn np",
    "test": "yarn run-s build:clean:launch test:sandbox clean",
    "test:sandbox": "cd sandbox && run-s lint test e2e build",
    "test:schematic": "npm run build && jasmine src/**/*_spec.js",
